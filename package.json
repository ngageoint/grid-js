    "displayName": "Grid Javascript",
    "description": "Grid Javascript",
    "keywords": [
        "NGA",
        "Grid"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ngageoint/grid-js.git"
    },
    "author": {
        "name": "NGA"
    },
    "contributors": [
        {
            "name": "Kevin Gilland",
            "email": "kgilland@caci.com"
        }
    ],
    "homepage": "https://www.nga.mil",
    "engines": {
        "npm": ">= 8.x"
    },
    "license": "MIT",
    "main": "index.ts",
    "dependencies": {
        "@ngageoint/color-js": "^2.0.1",
        "@ngageoint/simple-features-js": "1.1.1-beta.0",
        "properties-file": "^2.1.6",
        "ts-treemap": "^1.1.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/chai": "^4.3.3",
        "@types/mocha": "9.1.1",
        "chai": "4.3.6",
        "coveralls": "3.1.1",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.8.3"
    },
    "scripts": {
        "clean": "rm -rf dist; rm -rf .test_run",
        "test:run": "mocha --config .mocharc.js",
        "test": "npm run test:run",
        "compile": "tsc",
        "build": "npm run compile",
        "report": "nyc report",
        "coverage": "nyc npm run test:run",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "tslint -p tsconfig.json",
        "prebuild": "npm run clean",
        "pretest": "npm run build",
        "prepublishOnly": "npm run build && npm run lint && ts-node build/dist"
    },
    "files": [
        "dist"
    ],
    "nyc": {
        "temp-dir": "./.test_run",
        "report-dir": "./docs/coverage",
        "reporter": [
            "lcov"
        ]
    }
}
